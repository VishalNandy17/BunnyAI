{
  "name": "bunnyai-pro",
  "displayName": "BunnyAI Pro",
  "description": "Production-Ready VS Code Extension with Advanced AI & API Tools",
  "version": "0.0.1",
  "publisher": "bunnyai",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "bunnyai.runApi",
        "title": "Run API",
        "category": "BunnyAI"
      },
      {
        "command": "bunnyai.generateTests",
        "title": "Generate Tests",
        "category": "BunnyAI"
      },
      {
        "command": "bunnyai.analyzeError",
        "title": "Analyze Error",
        "category": "BunnyAI"
      },
      {
        "command": "bunnyai.generateDocs",
        "title": "Generate Documentation",
        "category": "BunnyAI"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bunnyai-sidebar",
          "title": "BunnyAI",
          "icon": "media/icons/bunny.svg"
        }
      ]
    },
    "views": {
      "bunnyai-sidebar": [
        {
          "id": "bunnyai.views.requests",
          "name": "Requests"
        },
        {
          "id": "bunnyai.views.history",
          "name": "History"
        },
        {
          "id": "bunnyai.views.collections",
          "name": "Collections"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "dev": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "build": "npm run package",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^22.0.0",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  }
}